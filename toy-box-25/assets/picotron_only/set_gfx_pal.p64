--[[pod_format="raw",created="2025-04-05 15:16:09",modified="2025-12-19 22:15:37",revision=11]]
-- get the process id of gfx.p64
local gfx_id = nil
for process in all(fetch"/ram/system/processes.pod") do
    if process.prog == "/system/apps/gfx.p64" then
        gfx_id = process.id
        break
    end
end
if gfx_id == nil then
    error"error: /system/apps/gfx.p64 not found in process list"
end

send_message(gfx_id,{event="set_palette",palette=fetch"toybox_palette.pal"})
